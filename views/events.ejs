<%- include("header", {active: "events"}); %>
    <div class="books row" style="margin-top: 12%">
      <% for(var i = 0; i < vagdevi_events.length; i++) {%>
        
        <div class="book card col-12 col-md-4"  style="text-align: center; width: 30%; height: 80%;background-color: blue;">
          <div class="card-body ">
            <img src="data:image/<%=vagdevi_events[i].img.contentType%>;base64,<%=vagdevi_events[i].img.data.toString('base64')%>" height="60%" width="60%">
            <h5>Student Name: <%=vagdevi_events[i].studentname %></h5>
            <h5>Event name: <%= vagdevi_events[i].eventname %></h5>
            <h5>Description: <%= vagdevi_events[i].description %></h5>
            <h5>Event date: <%= vagdevi_events[i].eventdate %></h5>
          </div>
        </div>
      <% } %>
      </div>

      <%- include('footer'); -%>
  
</script>
